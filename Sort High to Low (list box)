List<int> numbers = new List<int>() { 5, 3, 9, 1, 7 };

numbers.Sort(); // sort the list in ascending order
numbers.Reverse(); // reverse the list to get descending order

listBox1.Items.Clear(); // clear the list box

foreach (int num in numbers)
{
    listBox1.Items.Add(num); // add each number to the list box
}
